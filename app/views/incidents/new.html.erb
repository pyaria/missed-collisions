<%= simple_form_for @incident do |incident| %>
  <%= incident.input :date_time %>

  <%= incident.input :phone_email %>
  <%= incident.input :location_1 %>
  <%= incident.input :location_2 %>
  <%= link_to "Map", root_path %>
  <%= incident.input :license %>
  <%= incident.input :you %>
  <%= incident.input :them %>
  <%= incident.input :incident_type %>
  <%= incident.input :details %>
  <%= incident.submit %>

<% end %>
